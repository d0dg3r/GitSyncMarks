{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "short_name": "GitSyncMarks",
  "version": "2.1.0",
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "homepage_url": "https://github.com/d0dg3r/GitSyncMarks",
  "permissions": [
    "bookmarks",
    "storage",
    "alarms"
  ],
  "host_permissions": [
    "https://api.github.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
